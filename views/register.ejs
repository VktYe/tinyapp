<!-- Create registration form that includes form with:
 - email (type=email, name=email)
 - password field (type=password, name password)
 should POST ro /register -->

 <!DOCTYPE html>
 <html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
    name="viewport"
    content="width=device-width, initial-scale, shrink-to-fit=no"
    />
   
    <title>Registration</title>
  </head>
 
  <body>
    
    <h1>Register</h1>
    <form method="POST" action="/register">
      <div>
        <label for="email">Email:</label>
        <input 
          type="text" 
          id="email" 
          name="email"
        >
      </div>
      <div>
        <label for="password">Password:</label>
        <input 
          type="password"
          id="password"
          name="password"
        >
      </div>
      <button>Register</button>
    </form>
    <p> <a href="#"></a></p>
  </body>
 </html>